{
  "dashboard": {
    "title": "MCP Execution Monitoring",
    "tags": ["mcp", "execution", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Execution Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(mcp_executions_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Executions/sec"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "singlestat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(mcp_executions_total{status=\"success\"}[5m])) / sum(rate(mcp_executions_total[5m]))",
            "refId": "A"
          }
        ],
        "format": "percentunit",
        "valueName": "current",
        "thresholds": "0.8,0.95",
        "colors": ["#d44a3a", "#e0b400", "#299c46"]
      },
      {
        "id": 3,
        "title": "Active Executions",
        "type": "singlestat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(mcp_active_executions)",
            "refId": "A"
          }
        ],
        "format": "short",
        "valueName": "current",
        "thresholds": "50,100",
        "colors": ["#299c46", "#e0b400", "#d44a3a"]
      },
      {
        "id": 4,
        "title": "Execution Duration (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(mcp_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(mcp_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(mcp_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 5,
        "title": "Error Rate by Tool",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(mcp_execution_errors_total[5m])) by (tool_name, error_type)",
            "legendFormat": "{{tool_name}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 6,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(mcp_queue_depth) by (priority)",
            "legendFormat": "Priority {{priority}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Queue Depth"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 7,
        "title": "Queue Wait Time (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(mcp_queue_wait_time_seconds_bucket[5m])) by (priority, le))",
            "legendFormat": "Priority {{priority}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Wait Time"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "singlestat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "mcp_cache_hit_rate",
            "refId": "A"
          }
        ],
        "format": "percentunit",
        "valueName": "current",
        "thresholds": "0.5,0.8",
        "colors": ["#d44a3a", "#e0b400", "#299c46"]
      },
      {
        "id": 9,
        "title": "CPU Usage by Tool",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "sum(mcp_cpu_usage) by (tool_name)",
            "legendFormat": "{{tool_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "CPU Cores"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 10,
        "title": "Memory Usage by Tool",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "sum(mcp_memory_usage_mb) by (tool_name)",
            "legendFormat": "{{tool_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      },
      {
        "id": 11,
        "title": "Top 10 Tools by Execution Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "topk(10, sum(rate(mcp_executions_total[1h])) by (tool_name))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "styles": [
          {
            "pattern": "tool_name",
            "type": "string",
            "alias": "Tool Name"
          },
          {
            "pattern": "Value",
            "type": "number",
            "alias": "Executions/sec",
            "decimals": 2
          }
        ]
      },
      {
        "id": 12,
        "title": "Top 10 Users by Execution Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "topk(10, sum(rate(mcp_executions_total[1h])) by (user_id))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "styles": [
          {
            "pattern": "user_id",
            "type": "string",
            "alias": "User ID"
          },
          {
            "pattern": "Value",
            "type": "number",
            "alias": "Executions/sec",
            "decimals": 2
          }
        ]
      },
      {
        "id": 13,
        "title": "WebSocket Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ],
        "legend": {"show": true}
      },
      {
        "id": 14,
        "title": "API Response Time (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (endpoint, le))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "values": true, "current": true}
      }
    ]
  }
}
